{
  "parser": "babel-eslint",
  "root": true,
  "plugins": [],
  "env": {
    "browser": true,
    "commonjs": true,
    "jquery": true,
    "es6": true,
    "mocha": true
  },
  "settings": {
    "ecmascript": 6
  },
  "rules": {

  // Possible Errors
    "comma-dangle": [1, "never"],
    "no-cond-assign": [1, "always"],
    "no-console": 0,
    "no-constant-condition": 1,
    "no-control-regex": 0,
    "no-debugger": 0,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 0,
    "no-extra-boolean-cast": 0,
    "no-extra-parens": [1, "functions"],
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "use-isnan": 2,
    "valid-jsdoc": 1,
    "valid-typeof": 1,
    "no-unexpected-multiline": 1,

  // Best Practices
    "accessor-pairs": 0,
    "block-scoped-var": 0,
    "complexity": 0,
    "consistent-return": 0,
    "curly": 1,
    "default-case": 0,
    "dot-notation": 1,
    "dot-location": [1, "property"],
    "eqeqeq": [1, "smart"],
    "guard-for-in": 0,
    "no-aert": 0,
    "no-caller": 0,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-eq-null": 0,
    "no-eval": 0,
    "no-extend-native": 0,
    "no-extra-bind": 0,
    "no-fallthrough": 0,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 0,
    "no-implied-eval": 0,
    "no-invalid-this": 0,
    "no-iterator": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-multi-spaces": 1,
    "no-multi-str": 0,
    "no-native-reassign": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-new": 1,
    "no-octal-escape": 0,
    "no-octal": 0,
    "no-param-reassign": 0,
    "no-process-env": 0,
    "no-proto": 1,
    "no-redeclare": 1,
    "no-return-assign": [1, "always"],
    "no-script-url": 0,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 0,
    "no-unused-expressions": [1, { allowShortCircuit: true, allowTernary: true }],
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-with": 1,
    "radix": 0,
    "vars-on-top": 0,
    "wrap-iife": 1,
    "yoda": [1, "never", { "exceptRange": true }],

  // Variables
    "init-declarations": [1, "never", { "ignoreForLoopInit": true }],
    "no-catch-shadow": 0,
    "no-delete-var": 1,
    "no-label-var": 0,
    "no-shadow-restricted-named": 0,
    "no-shadow": 1,
    "no-undef-init": 1,
    "no-undef": 1,
    "no-undefined": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,

  // Node.js and CommonJS
    "callback-return": 0,
    "global-require": 1,
    "handle-callback-err": 0,
    "no-mixed-required": 0,
    "no-new-require": 0,
    "no-path-concat": 0,
    "no-process-exit": 0,
    "no-restricted-modules": 0,
    "no-sync": 0,

  // Stylistic Issues
    "array-bracket-spacing": [1, "never"],
    "block-spacing": [1, "always"],
    "brace-style": [1, "1tbs", { "allowSingleLine": true }],
    "camelcase": 1,
    "comma-spacing": [1, {"before": false, "after": true}],
    "comma-style": [1, "last"],
    "computed-property-spacing": [1, "never"],
    "consistent-this": 0,
    "eol-last": 1,
    "func-names": 0,
    "func-style": 0,
    "id-length": 0,
    "id-match": 0,
    "indent": [2, 2, { "VariableDeclarator": { "var": 2, "let": 2, "const": 3 }, "SwitchCase": 1 }],
    "jsx-quotes": [1, "prefer-single"],
    "key-spacing": [1, {"beforeColon": false, "afterColon": true}],
    "lines-around-comment": 0,
    "linebreak-style": 0,
    "max-nested-callbacks": 0,
    "new-cap": 1,
    "new-parens": 1,
    "newline-after-var": 1,
    "no-array-constructor": 1,
    "no-continue": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": [1, "smart-tabs"],
    "no-multiple-empty-lines": 1,
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-restricted-syntax": 0,
    "no-spaced-func": 1,
    "no-ternary": 0,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 1,
    "object-curly-spacing": [1, "always"],
    "one-var": 0,
    "operator-assignment": [1, "always"],
    "operator-linebreak": 0,
    "padded-blocks": [1, "never"],
    "quote-props": [1, "as-needed"],
    "quotes": [1, "single", "avoid-escape"],
    "require-jsdoc": 0,
    "semi-spacing": [1, { "before": false, "after": true }],
    "semi": [1, "always"],
    "sort-vars": 0,
    "space-after-keywords": [1, "always"],
    "space-before-keywords": [1, "always"],
    "space-before-blocks": [1, { "functions": "never", "keywords": "always" }],
    "space-before-function-paren": [1, "never"],
    "space-in-parens": [1, "never"],
    "space-infix-ops": 1,
    "space-return-throw-case": 1,
    "space-unary-ops": 0,
    "spaced-comment": 0,
    "wrap-regex": 0,

  // ES6
    "arrow-parens": [0, "as-needed"],
    "arrow-spacing": [1, { "before": true, "after": true }],
    "constructor-super": 2,
    "generator-star-spacing": 0,
    "no-class-assign": 1,
    "no-const-assign": 2,
    "no-dupe-class-members": 1,
    "no-this-before-super": 1,
    "no-var": 1,
    "object-shorthand": [1, "always"],
    "prefer-arrow-callback": 0,
    "prefer-const": 1,
    "prefer-spread": 1,
    "prefer-reflect": 0,
    "prefer-template": 1,
    "require-yield": 0
  },

  // Whitelisted ES Features
  "ecmaFeatures": {
    "arrowFunctions": true,
    "binaryLiterals": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "octalLiterals": true,
    "regexUFlag": true,
    "regexYFlag": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": false,
    "globalReturn": false,
    "jsx": true,
    "experimentalObjetRestSpread": false
  }

}

